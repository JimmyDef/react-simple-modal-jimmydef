{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src", "README.md"],
    "exclude": ["node_modules"]
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/docdash"
  },
  "templates": {
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    },
    "docdash": {
      "static": true,
      "sort": true
    }
  }
}
